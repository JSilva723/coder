{{#if isValid}}
<div>
    {{#each payload}}
    <div>
        <p>{{this.title}}</p>
    </div>
    <br/>
    {{/each}}
</div>
<br/>
<div>
    {{#if hasPrevPage}}
        <a href={{this.prevLink}}><< Anterior</a>
    {{/if}}
    &nbsp;
    {{this.page}}
    &nbsp;
     {{#if hasNextPage}}
        <a href={{this.nextLink}}>Siguiente >></a>
    {{/if}} 
</div>
{{else}}
<h1>No hay productos para mostrar</h1>
{{/if}}
<script src="/socket.io/socket.io.js"></script>
<script src="/js/{{script}}"></script>